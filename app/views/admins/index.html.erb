<%= simple_form_for User.new do |f| %>
  <%= f.input :name %>
  <%= f.button :submit %>
<% end %>

<% if @admins.count > 0 %>
<table class="table table-hover table-striped" >
  <thead>
    <tr>
      <th>Name</th>
      <th>UID</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
  <% @admins.each do |a| %>
    <tr>
      <td><%= a.nickname %></td>
      <td><%= a.uid %></td>
      <td><%= button_to "delete", a, method: :delete %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<% end %>
