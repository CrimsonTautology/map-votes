<% users.each do |user| %>
  <div class="col-md-2 thumbnail-wrapper">
    <%= link_to  user.profile_url do %>
      <%= image_tag user.avatar_url, alt: user.nickname, class: "img-rounded user-thumbnail" %>
      <p class="label label-inverse thumbnail-text">
        <%= user.nickname %>
      </p>
    <% end %>
  </div>
<% end %>
